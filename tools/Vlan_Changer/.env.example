# VLAN Changer Tool Environment Variables
# Copy this to .env and fill in your actual values

# ====================
# PRIMARY CREDENTIALS
# ====================
# Primary switch credentials
PRIMARY_USERNAME=your_primary_username
PRIMARY_PASSWORD=your_primary_password

# ====================
# FALLBACK CREDENTIALS
# ====================
# Multiple fallback credential sets (remove unused ones)
FALLBACK_USER1=fallback_user1
FALLBACK_PASS1=fallback_pass1
FALLBACK_SECRET1=fallback_enable1

FALLBACK_USER2=fallback_user2
FALLBACK_PASS2=fallback_pass2
FALLBACK_SECRET2=fallback_enable2

FALLBACK_USER3=fallback_user3
FALLBACK_PASS3=fallback_pass3
FALLBACK_SECRET3=fallback_enable3

FALLBACK_USER4=fallback_user4
FALLBACK_PASS4=fallback_pass4
FALLBACK_SECRET4=fallback_enable4

# ====================
# VLAN NETWORK LOCATIONS
# ====================
# Define your network locations for VLAN operations (up to 20 supported)
VLAN_LOCATION_1_NAME=Site1_Core
VLAN_LOCATION_1_IP=192.168.1.1

VLAN_LOCATION_2_NAME=Site2_Core
VLAN_LOCATION_2_IP=192.168.1.2

